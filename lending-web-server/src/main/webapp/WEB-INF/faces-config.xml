<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>showAllGames</from-outcome>
			<to-view-id>/games/show-games.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>showAllGameInfos</from-outcome>
			<to-view-id>/games/show-game-infos.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>activateGame</from-outcome>
			<to-view-id>/games/activate-game.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>deactivateGame</from-outcome>
			<to-view-id>/games/deactivate-game.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>addSingleGame</from-outcome>
			<to-view-id>/games/add-single-game.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>showAllIdentityCards</from-outcome>
			<to-view-id>/identitycards/show-identitycards.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>setIdentityCardOwner</from-outcome>
			<to-view-id>/identitycards/set-owner.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>activateIdentityCard</from-outcome>
			<to-view-id>/identitycards/activate-identitycard.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>deactivateIdentityCard</from-outcome>
			<to-view-id>/identitycards/deactivate-identitycard.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>showAllEnvelopes</from-outcome>
			<to-view-id>/envelopes/show-envelopes.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>activateEnvelope</from-outcome>
			<to-view-id>/envelopes/activate-envelope.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>deactivateEnvelope</from-outcome>
			<to-view-id>/envelopes/deactivate-envelope.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>statusOverview</from-outcome>
			<to-view-id>/ausleihe/overview.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>statistics</from-outcome>
			<to-view-id>/ausleihe/transaction-statistics.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>todaysStatistics</from-outcome>
			<to-view-id>/ausleihe/todays-transaction-statistics.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>lendGamegroups</from-outcome>
			<to-view-id>/ausleihe/lend-games-statistics.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>gamesStatus</from-outcome>
			<to-view-id>/ausleihe/games-status.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>identitycardsStatus</from-outcome>
			<to-view-id>/ausleihe/identitycards-status.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>gamesPerIdCard</from-outcome>
			<to-view-id>/ausleihe/games-per-idcard.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>emptyDatabase</from-outcome>
			<to-view-id>/admin/empty-database.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>mobile</from-outcome>
			<to-view-id>/mobile/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<application />
</faces-config>
